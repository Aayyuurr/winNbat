{
	"useTabs": true,
	"singleQuote": true,
	"printWidth": 100,
	"plugins": ["prettier-plugin-svelte"],
	"pluginSearchDirs": ["."],
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }],
	"arrowParens": "always",
	"quoteProps": "consistent",
	"semi": true,
	"trailingComma": "es5",
	"vueIndentScriptAndStyle": true
}
